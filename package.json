    "@babel/preset-env": "^7.12.1",
    "@types/bunyan": "^1.8.8",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-loader": "^8.1.0",
